    "ace-builds": "^1.34.2",
    "crawler-user-agents": "^1.0.142",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "next": "14.2.4",
    "nodemailer-sendgrid": "^1.0.3",
    "payload": "2.22.0",
    "react": "^18.3.1",
    "react-ace": "^11.0.1",
