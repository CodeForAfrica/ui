  "version": "0.1.26",
  "private": true,
  "author": "Code for Africa <tech@codeforafrica.org>",
  "description": "This is the official code for https://charter.africa site",
  "keywords": [
    "africa",
    "charter",
    "next",
    "next.js",
    "react",
    "mui"
