  "scripts": {
    "build:next": "next build",
    "build:payload": "payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "NODE_ENV=production pnpm build:payload && pnpm build:server && PAYLOAD_CONFIG_PATH=dist/payload.config.js NEXT_BUILD=true node dist/server.js",
    "dev": "NODE_OPTIONS='--inspect' ts-node --project tsconfig.server.json server.ts",
    "start": "PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "lint-check": "eslint './'",
    "lint": "eslint --fix './'",
    "jest": "jest",
    "playwright": "npx playwright test",
    "clean": "rm -rf .next .turbo build dist node_modules"
  },
