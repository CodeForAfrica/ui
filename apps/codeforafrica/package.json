    "@types/express": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "babel-jest": "catalog:",
    "eslint": "catalog:",
    "eslint-config-commons-ui": "workspace:*",
    "eslint-import-resolver-webpack": "catalog:",
