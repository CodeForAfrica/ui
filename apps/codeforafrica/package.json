{
  "name": "codeforafrica",
  "version": "1.0.45",
  "private": true,
  "author": "Code for Africa <tech@codeforafrica.org>",
  "description": "This is the main CFA site.",
  "keywords": [
    "codeforafrica",
    "next",
    "next.js",
    "react",
    "mui"
  ],
  "repository": {
    "type": "git",
