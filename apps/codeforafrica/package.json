    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "jsdom": "^21.1.2",
    "marked": "^4.3.0",
    "nanoid": "^4.0.2",
    "next": "~13.4.11",
    "next-seo": "^5.15.0",
    "nodemailer-sendgrid": "^1.0.3",
    "payload": "^1.7.5",
