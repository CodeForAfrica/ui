{
  "name": "ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write --ignore-path './.gitignore' \"**/*.+(json|md|yml)\" './'"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "turbo": "1.1.6"
  },
  "packageManager": "pnpm@6.31.0"
}
